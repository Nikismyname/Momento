{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.1",
    "configurations": [
        {
            "type": "chrome",
            "request": "launch",
            "name": "Launch Chrome against localhost",
            "url": "https://localhost:44307",
            "sourceMaps": true,
            "webRoot": "${workspaceFolder}/Momento.Web/wwwroot",
            "sourceMapPathOverrides": {
                "webpack:///*": "${workspaceFolder}/Momento.Web/wwwroot/ReactApps/*",
                "webpack:///./*": "${workspaceFolder}/Momento.Web*",
                "webpack:///ReactSource/*": "${workspaceFolder}/Momento.Web/ReactSource/*",
                "webpack:///./~/*": "${workspaceFolder}/Momento.Web/node_modules/*"
            },
        }
    ]
}